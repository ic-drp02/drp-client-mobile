{
  "expo": {
    "name": "ICON",
    "slug": "drp-client-mobile",
    "platforms": ["ios", "android", "web"],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon_logo_opaque.png",
    "splash": {
      "image": "./assets/icon_full.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "uk.ac.imperial.IconGuidelines"
    },
    "android": {
      "package": "com.ic_drp02.icon_guidelines",
      "versionCode": 1
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "userInterfaceStyle": "automatic"
  }
}
