{
  "expo": {
    "name": "drp-client-mobile",
    "slug": "drp-client-mobile",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_logo.png",
    "splash": {
      "image": "./assets/icon_full.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ic_drp02.nhs_guidelines",
      "versionCode": 1
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    }
  }
}
